{
  "version": 2,
  "name": "native-server",
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "builds/sih/**/*", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/builds/sih/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
